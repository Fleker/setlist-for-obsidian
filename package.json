		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "5.6.3"
	},
	"dependencies": {
		"setlistfm-js": "^1.2.0"
	}
